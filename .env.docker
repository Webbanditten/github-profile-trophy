# GitHub Profile Trophy - Docker Environment Configuration
# Copy this file to .env and fill in your values

# Server Port (default: 8080)
PORT=8080

# GitHub API Tokens (you can use multiple tokens for higher rate limits)
# Get your token at: https://github.com/settings/tokens
GITHUB_TOKEN1=your_github_token_here
GITHUB_TOKEN2=

# GitHub API Endpoint
GITHUB_API=https://api.github.com/graphql

# Redis Configuration
ENABLE_REDIS=true
REDIS_PORT=6379
REDIS_HOST=redis
REDIS_USERNAME=
REDIS_PASSWORD=
